[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
req_extensions     = req_ext

[ dn ]
C  = US
ST = Example State
L  = Example City
O  = Example Organization
OU = Client Certificate
CN = Test User Local cert

[ req_ext ]
keyUsage = digitalSignature
extendedKeyUsage = clientAuth
subjectAltName = DNS:localhost